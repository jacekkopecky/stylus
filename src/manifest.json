{
  "name": "Stylus-without-network",
  "version": "0.0.1",
  "minimum_chrome_version": "",
  "description": "__MSG_description__",
  "homepage_url": "https://github.com/jacekkopecky/stylus-without-network",
  "manifest_version": 0,
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "permissions": [],
  "background": {},
  "commands": {
    "openManage": { "description": "__MSG_openManage__" },
    "reload": { "description": "__MSG_reload__" },
    "styleDisableAll": { "description": "__MSG_disableAllStyles__" }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "all_frames": true,
      "match_about_blank": true,
      "match_origin_as_fallback": true,
      "js": ["js/apply.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon/16w.png",
      "32": "icon/32w.png",
      "19": "icon/19w.png",
      "38": "icon/38w.png"
    },
    "default_title": "Stylus",
    "default_popup": "popup.html"
  },
  "default_locale": "en"
}
